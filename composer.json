{
    "name": "thewebmen/silverstripe-structureddata",
    "description": "Easy way to add structured data to your silverstripe website using json-ld",
    "type": "silverstripe-vendormodule",
    "keywords": [
        "silverstripe",
        "structured",
        "data",
        "json-ld"
    ],
    "license": "BSD-3-Clause",
    "authors": [{
        "name": "Webmen",
        "email": "development@webmen.nl"
    }],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "silverstripe/cms": "^4.0@dev",
        "silverstripe/vendor-plugin": "^1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.7",
        "squizlabs/php_codesniffer": "^3.0"
    },
    "extra": {
        "installer-name": "silverstripe-structureddata",
        "branch-alias": {
            "dev-master": "1.x-dev"
        }
    },
    "autoload": {
        "psr-4": {
            "Webmen\\StructuredData\\": "src/"
        }
    }
}
